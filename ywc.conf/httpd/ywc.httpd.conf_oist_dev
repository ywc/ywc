NameVirtualHost *:80

<VirtualHost *:80>
 ServerName www.oist.ywc.me
 DocumentRoot /var/www/html/oist-www
</VirtualHost>

#<VirtualHost *:80>
# ServerName tida.oist.ywc.me
# DocumentRoot /var/www/html/oist-tida
#</VirtualHost>


<VirtualHost *:80>
  ServerName tida.oist.ywc.me
  DocumentRoot /var/www/html/oist-tida

  ProxyPass         /bws  http://localhost:8081/bws
  ProxyPassReverse  /bws  http://localhost:8081/bws

  RewriteEngine on
  RewriteCond    %{REQUEST_URI} !/lib
  RewriteCond    %{REQUEST_URI} !/bws
  RewriteRule ^(.*)$ http://localhost:8081/fws/router?uri=$1 [P,QSA]
 
  Alias /lib /data/ywc/ywc.lib
  <Directory /data/ywc/ywc.lib>
    Order allow,deny
    Allow from all
    Options +Indexes
</Directory>

</VirtualHost>


<VirtualHost *:80>
  ServerName www.ywc.me
  ServerAlias *.ywc.me
  DocumentRoot /var/www/html
</VirtualHost>

<Directory /var/www/html/oist-www>
   AuthType Basic
   AuthName "Restricted Access"
   AuthUserFile /etc/httpd/passwd
   Require valid-user
   AllowOverride All
</Directory>
<Directory /var/www/html/oist-tida>
   AuthType Basic
   AuthName "Restricted Access"
   AuthUserFile /etc/httpd/passwd
   Require valid-user
</Directory>



